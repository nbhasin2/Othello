<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="othello.hypercube.kernelpanic.myapplication" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="othello.hypercube.kernelpanic.myapplication.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="othello.hypercube.kernelpanic.myapplication.OthelloGrid"
            android:label="@string/title_activity_othello_grid"
            android:parentActivityName="othello.hypercube.kernelpanic.myapplication.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="othello.hypercube.kernelpanic.myapplication.MainActivity" />
        </activity>
    </application>

</manifest>
